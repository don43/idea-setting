<application>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="ide Default" />
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseOneLineMethods" value="false" />
    <option name="collapseSuppressWarnings" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>